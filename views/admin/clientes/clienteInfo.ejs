<%- include('../../partials/admin/header.ejs') %>
<main class="principal container">
  <hgroup>
    <h2>Cliente</h2>
    <h3>Informações do cliente</h3>
  </hgroup>
  <hr class="horizontal-bar">
  <article class="cliente-info">
    Nome:
    <p><%= cliente.nome %></p>

    Data de nascimento/Criação:
    <p><%= cliente.nascimento %></p>

    Email:
    <p><%= cliente.email %></p>

    Telefone:
    <p><%= cliente.telefone %></p>

    Celular:
    <p><%= cliente.celular %></p>

    CEP:
    <p><%= cliente.cep %></p>

    UF:
    <p><%= cliente.uf %></p>

    Número do estabelecimento:
    <p><%= cliente.numero %></p>

    Localização/Cidade:
    <p><%= cliente.localizacao %></p>

    Endereço:
    <p><%= cliente.endereco %></p>

    Informações adicionais:
    <p><%= cliente.informacoes_adicionais %></p>

    CPF/CNPJ:
    <p><%= cliente.identificacao %></p>

    Contrato assinado:
    <input type="file" name="iptContrato">

    <a href="/admin/clientes" class="btn btn-info btn-right">Voltar</a>
  </article>
</main>
<%- include('../../partials/admin/footer.ejs') %>