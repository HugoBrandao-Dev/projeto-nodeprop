<%- include('../../partials/admin/header.ejs') %>
<main class="principal container">
  <hgroup>
    <h2>Serviço</h2>
    <h3>Informações do serviço</h3>
</hgroup>
<hr class="horizontal-bar">
<article class="servico-info">
    Serviço:
    <p><%= resultado.servico %></p>

    Breve descrição:
    <p><%= resultado.breve_descricao %></p>

    Responsáveis:
    <ul class="ul-responsaveis">
      <% resultado.responsaveis.forEach(responsavel => { %>
        <li class="lbl-func-cargo"><%= responsavel.nome %></li>
      <% }) %>
    </ul>

    Informações adicionais:
    <p><%= resultado.informacoes %></p>

    Contrato do serviço:
    <p>Arquivo de contrato.</p>

    <a href="/admin/servicos" class="btn btn-info btn-right">Voltar</a>
</article>
</main>
<%- include('../../partials/admin/footer.ejs') %>